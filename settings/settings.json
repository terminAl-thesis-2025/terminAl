{
  "sudo": true,
  "prev_hallucination": false,
  "os_mapping": {
    "tree_command": [
      "tree",
      "/",
      "-f",
      "-J",
      "--nolinks",
      "-I",
      "proc|sys|dev|run|mnt|media|tmp|var/tmp|var/cache|dist-packages|site-packages|__pycache__|test|tests|scipy",
      "-o"
    ],
    "delete_tree_command": [
      "rm"
    ],
    "tree_file_path": "database/system_tree.json"
  },
  "ollama_settings": {
    "ollama_url": "http://host.docker.internal:11434",
    "ollama_model": "hf.co/terminAl-thesis-2025/Llama3.1-terminAl-8b-q5_k_m:Q5_K_M",
    "modelloptionen": {
      "1": "hf.co/terminAl-thesis-2025/Llama3.1-terminAl-8b-q5_k_m:Q5_K_M",
      "2": "hf.co/terminAl-thesis-2025/Llama3.1-terminAl-8b:Q5_K_M"
    }
  },
  "guard_settings": {
    "guard_model": "terminAl-thesis-2025/deberta-v3-base-terminAl-guard"
  },
  "chroma_settings": {
    "chromadb_path": "./database/vector_db",
    "chromadb_tree_collection": "tree_collection",
    "chromadb_docs_collection": "docs_collection",
    "chroma_update_interval": 600,
    "chroma_auto_update": false,
    "chroma_latest_update": "22.05.2025 09:14:04",
    "embedding_model": "intfloat/multilingual-e5-small",
    "collection_archive_size": 5
  },
  "tools": {
    "postgres": {
      "username": "postgres",
      "databases": [
        "test"
      ],
      "mapping_tables_command2": [
        "psql",
        "-U",
        "<username>",
        "-d",
        "<database>",
        "-c",
        "\\dt"
      ],
      "mapping_tables_command": [
        "sudo",
        "-u",
        "postgres",
        "psql",
        "-d",
        "<database>",
        "-c",
        "\\dt"
      ]
    }
  },
  "model_cache_directory": "model_cache",
  "chroma_auto_update": false
}